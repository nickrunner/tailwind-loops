{
  "name": "tailwind-loops",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "clean": "pnpm -r clean",
    "typecheck": "pnpm -r typecheck",
    "test": "pnpm -r test",
    "tuner": "pnpm --filter @tailwind-loops/tuner dev",
    "serve": "pnpm --filter @tailwind-loops/server start",
    "dev": "pnpm --filter @tailwind-loops/server --filter @tailwind-loops/tuner -r --parallel dev"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "pnpm": "^10.29.3"
  }
}
